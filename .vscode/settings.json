{
  "[html][javascript][javascriptreact][typescript][typescriptreact][json][jsonc][css][tailwindcss]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.biome": "explicit",
      "source.fixAll.ts": "explicit"
    },
    "editor.defaultFormatter": "biomejs.biome",
    "editor.formatOnSave": true
  },
  "eslint.enable": false,
  "i18n-ally.localesPaths": ["messages"],
  "json.schemas": [
    {
      "fileMatch": ["bun.lock"],
      "url": "https://raw.githubusercontent.com/oven-sh/bun/main/packages/bun-types/lockfile.json"
    }
  ],
  "prettier.enable": false
}
