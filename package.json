  },
  "name": "finance-manager",
  "private": true,
  "scripts": {
    "build": "next build",
    "check": "bun run check:biome && bun run check:i18n && bun run check:types",
    "check:biome": "bunx -b biome check",
    "check:i18n": "i18n-check --source en --locales messages --unused src",
    "check:types": "tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "fix": "bunx -b biome check --write .",
    "postinstall": "bun run db:generate",
    "preview": "next build && next start",
    "start": "node .next/standalone/server.js"
  },
  "type": "module",
  "version": "0.1.0"
