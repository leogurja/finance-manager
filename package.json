{
  "name": "finance-manager",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "pnpm check:types && pnpm check:i18n && pnpm check:lint && pnpm check:format",
    "check:format": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "check:i18n": "i18n-check --source en --locales messages --unused src",
    "check:lint": "eslint --fix",
    "check:types": "tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "fix": "pnpm lint:fix && pnpm format:fix",
    "fix:format": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "fix:lint": "eslint --fix",
    "postinstall": "prisma generate",
    "preview": "next build && next start",
    "start": "node .next/standalone/server.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.5.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "neverthrow": "^8.2.0",
    "next": "^15.2.3",
    "next-intl": "^4.3.12",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.65.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.24.2",
    "zsa": "^0.6.0",
    "zsa-react": "^0.2.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@gurja/eslint-config": "^6.0.0",
    "@lingual/i18n-check": "^0.8.11",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/eslint": "^9.6.1",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "eslint-plugin-neverthrow": "^1.1.4",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.5.0",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "packageManager": "pnpm@10.15.1"
}
