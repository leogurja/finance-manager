{
  "name": "finance-manager",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "bun run /^check:/",
    "check:format": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "check:i18n": "i18n-check --source en --locales messages --unused src",
    "check:lint": "eslint --fix",
    "check:types": "tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "fix": "bun run /^fix:/",
    "fix:format": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "fix:lint": "eslint --fix",
    "postinstall": "bun run db:generate",
    "preview": "next build && next start",
    "start": "node .next/standalone/server.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.17.1",
    "@t3-oss/env-nextjs": "^0.13.8",
    "better-auth": "^1.3.27",
    "client-only": "^0.0.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.545.0",
    "neverthrow": "^8.2.0",
    "next": "15.5.4",
    "next-intl": "^4.3.12",
    "next-safe-action": "^8.0.11",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@gurja/eslint-config": "^6.0.0",
    "@lingual/i18n-check": "^0.8.11",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.4",
    "eslint-plugin-neverthrow": "^1.1.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.17.1",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  }
}
